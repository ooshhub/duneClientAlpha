{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": [
    "electron.vite.config.*",
    "src/main/*",
    "src/preload/*",
    "src/main.ts",
    "src/@types/*",
    "src/shared/*",
  ],
  "exclude": [
    "clientBuild/**/*",
    "build/**/*",
  ],
  "compilerOptions": {
    "composite": true,
    "allowJs": true,
    "outDir": "./clientBuild"
  }
}
